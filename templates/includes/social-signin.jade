.social-signup-buttons
	.signup-button: a(href='/auth/github?target=' + page.path, title='Sign in with Github').btn.btn-link-github.btn-block
	a.btn.btn-social-icon.btn-github
		span.fa.fa-github Sign in with Github

	.signup-button: a(href='/auth/twitter?target=' + page.path, title='Sign in with Twitter').btn.btn-link-twitter.btn-block
		a.btn.btn-social-icon.btn-twitter
			span.fa.fa-twitter Sign in with Twitter
